<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="442.0" prefWidth="685.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mylabproject.StudentLoginController">
    <children>
        <ImageView fitHeight="480.0" fitWidth="791.0" layoutX="-35.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/AdminPage.jpg" />
            </image>
        </ImageView>
        <AnchorPane layoutX="116.0" layoutY="166.0" prefHeight="231.0" prefWidth="445.0" style="-fx-background-color: white;">
            <children>
                <AnchorPane layoutX="-139.0" layoutY="-22.0" prefHeight="30.0" prefWidth="0.0" />
                <TextField editable="false" layoutX="132.0" layoutY="14.0" prefHeight="27.0" prefWidth="183.0" style="-fx-background-color: white;" text="Already have an account ?">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </TextField>
                <ImageView fitHeight="24.0" fitWidth="18.0" layoutX="76.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/AdminIcon.png" />
                    </image>
                </ImageView>
                <TextField fx:id="UserTf" layoutX="100.0" layoutY="56.0" prefHeight="26.0" prefWidth="227.0" promptText="Username" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 1px 0px;" />
                <ImageView fitHeight="24.0" fitWidth="18.0" layoutX="76.0" layoutY="93.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/bookpassword.png" />
                    </image>
                </ImageView>
                <Button fx:id="loginbt" layoutX="100.0" layoutY="129.0" mnemonicParsing="false" onAction="#LoginBt" prefHeight="30.0" prefWidth="234.0" style="-fx-background-color: #FA9C18;" text="Log in" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="16.0" />
               </font></Button>
            <PasswordField fx:id="passTf" layoutX="100.0" layoutY="89.0" prefHeight="26.0" prefWidth="227.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 1px 0px;" />
            </children>
        </AnchorPane>
      <Button fx:id="backbt" mnemonicParsing="false" onAction="#backBt" prefHeight="27.0" prefWidth="54.0" style="-fx-background-color: #FF6700;" text="BACK" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
    </children>
</AnchorPane>
